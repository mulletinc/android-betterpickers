#. [CHAR_LIMIT=NONE]
#, c-format
msgctxt "deleted_key"
msgid "%1$s deleted"
msgstr "%1$s deletado"

#. Repeat an monthly event on the same day of every month [CHAR LIMIT=20]
msgctxt "recurrence_month_pattern_by_day"
msgid "on the same day each month"
msgstr "todo dia a cada mês"

#. Description of the selected marker for accessibility support [CHAR LIMIT =
#. NONE]
msgctxt "acessibility_recurrence_choose_end_date_description"
msgid "change end date"
msgstr "mudar data final"

#. The button label for saving an event
msgctxt "save_label"
msgid "Done"
msgstr "Concluído"

#. Specifies that a repeating event to repeat forever (based on the defined
#. frequency) instead of ending at a future date[CHAR LIMIT=20]
msgctxt "recurrence_end_continously"
msgid "Forever"
msgstr "Para sempre"

#. Specifies that a repeating event to repeat until a certain date[CHAR
#. LIMIT=25]
msgctxt "recurrence_end_date_label"
msgid "Until a date"
msgstr "Até uma data"

#. Specifies that a repeating event to repeat until a certain date[CHAR
#. LIMIT=20]
#, c-format
msgctxt "recurrence_end_date"
msgid "Until <xliff:g id=\"date\">%s</xliff:g>"
msgstr "Até <xliff:g id=\"date\">%s</xliff:g>"

#. Specifies that a repeating event to repeat for a number of times. A
#. repeating event can repeat forever, repeat in a certain frequency until a
#. certain date, or repeat for a number of times. e.g. repeat 10 times and
#. stop. This string is for the last case [CHAR LIMIT=25]
msgctxt "recurrence_end_count_label"
msgid "For a number of events"
msgstr "Por um número de eventos"

#. Strings to describe how frequently to repeat an event (e.g. every 5 hours)
#, c-format
msgctxt "recurrence_interval_hourly"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> hour"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> hours"
msgstr[0] ""
msgstr[1] ""

#. Strings to describe how frequently to repeat an event (e.g. every 5 days)
#, c-format
msgctxt "recurrence_interval_daily"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> day"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> days"
msgstr[0] "A cada <xliff:g id=\"count\">%d</xliff:g> dia"
msgstr[1] "A cada <xliff:g id=\"count\">%d</xliff:g> dias"

#. Strings to describe how frequently to repeat an event (e.g. every 5 weeks)
#, c-format
msgctxt "recurrence_interval_weekly"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> week"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> weeks"
msgstr[0] "A cada <xliff:g id=\"count\">%d</xliff:g> semana"
msgstr[1] "A cada <xliff:g id=\"count\">%d</xliff:g> semanas"

#. Strings to describe how frequently to repeat an event (e.g. every 5 months)
#, c-format
msgctxt "recurrence_interval_monthly"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> month"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> months"
msgstr[0] "A cada <xliff:g id=\"count\">%d</xliff:g> mês"
msgstr[1] "A cada <xliff:g id=\"count\">%d</xliff:g> meses"

#. Strings to describe how frequently to repeat an event (e.g. every 5 years)
#, c-format
msgctxt "recurrence_interval_yearly"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> year"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> years"
msgstr[0] "A cada <xliff:g id=\"count\">%d</xliff:g> ano"
msgstr[1] "A cada <xliff:g id=\"count\">%d</xliff:g> anos"

#. Specifies that a repeating event should repeat for a certain number of
#. iterations. [CHAR LIMIT=20]
#, c-format
msgctxt "recurrence_end_count"
msgid "For <xliff:g id=\"event_count\">%d</xliff:g> event"
msgid_plural "For <xliff:g id=\"event_count\">%d</xliff:g> events"
msgstr[0] "Por <xliff:g id=\"event_count\">%d</xliff:g> evento"
msgstr[1] "Por <xliff:g id=\"event_count\">%d</xliff:g> eventos"

#. Part of a description of an repeating event. This part states the date when
#. a repeating          event ends. The part before the semi-colon describes
#. how it repeats.          e.g. Every 2 weeks; until 1/1/2030 [CHAR LIMIT=21]
#, c-format
msgctxt "endByDate"
msgid "; until <xliff:g id=\"date\">%s</xliff:g>"
msgstr "; até <xliff:g id=\"date\">%s</xliff:g>"

#. Part of a description of an repeating event. This part how many times the
#. event repeats.          The part before the semi-colon describes how it
#. repeats.          e.g. Every 2 weeks; for 15 times [CHAR LIMIT=15]
#, c-format
msgctxt "endByCount"
msgid "; for one time"
msgid_plural "; for <xliff:g id=\"number\">%d</xliff:g> times"
msgstr[0] "; uma vez"
msgstr[1] "; <xliff:g id=\"number\">%d</xliff:g> vezes"

#. A description for an event that repeats daily. For example, Every 2 days
#. [CHAR LIMIT=15]
#, c-format
msgctxt "hourly"
msgid "Hourly"
msgid_plural "Every <xliff:g id=\"number\">%d</xliff:g> hours"
msgstr[0] ""
msgstr[1] ""

#. A description for an event that repeats daily. For example, Every 2 days
#. [CHAR LIMIT=15]
#, c-format
msgctxt "daily"
msgid "Daily"
msgid_plural "Every <xliff:g id=\"number\">%d</xliff:g> days"
msgstr[0] "Diariamente"
msgstr[1] "A cada <xliff:g id=\"number\">%d</xliff:g> dias"

#. A description for an event that repeats weekly on some day of the week. For
#. example,          'Weekly on Tuesday' or Every 2 weeks on Monday, Tuesday.
#. [CHAR LIMIT=25]
#, c-format
msgctxt "weekly"
msgid "Weekly on <xliff:g id=\"days_of_week\">%2$s</xliff:g>"
msgid_plural ""
"Every <xliff:g id=\"number\">%1$d</xliff:g> weeks on <xliff:g "
"id=\"days_of_week\">%2$s</xliff:g>"
msgstr[0] "Toda semana, <xliff:g id=\"days_of_week\">%2$s</xliff:g>"
msgstr[1] ""
"A cada <xliff:g id=\"number\">%1$d</xliff:g> semanas, <xliff:g "
"id=\"days_of_week\">%2$s</xliff:g>"

#. The common portion of a string describing how often an event repeats,
#. example: 'Monthly (on day 2)'
msgctxt "monthly"
msgid "Monthly"
msgstr "Mensalmente"

#. Calendar spinner item, to select that an event recurs every year.
msgctxt "yearly_plain"
msgid "Yearly"
msgstr "Anualmente"

#. Hint text to encourage the user to enter a country name to find a time zone
#. [CHAR LIMIT=25]
msgctxt "hint_time_zone_search"
msgid "Type country name"
msgstr "Digite o nome do país"

#. Description of clear icon in the search bar, for accessibility. [CHAR
#. LIMIT=NONE]
msgctxt "searchview_description_clear"
msgid "Clear query"
msgstr "Limpar busca"

#. Item to show when no timezones are found. [CHAR LIMIT=NONE]
msgctxt "no_results_found"
msgid "No results found"
msgstr "Nenhum resultado encontrado"

