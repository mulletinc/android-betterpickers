#, c-format
msgctxt "recurrence_interval_hourly"
msgid "Every <xliff:g id=\"count\">%d</xliff:g> hour"
msgid_plural "Every <xliff:g id=\"count\">%d</xliff:g> hours"
msgstr[0] ""
msgstr[1] ""
msgstr[2] ""
msgstr[3] ""

