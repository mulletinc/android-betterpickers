msgid_plural "Every <xliff:g id=\"number\">%d</xliff:g> hours"
msgstr[0] ""
msgstr[1] ""
msgstr[2] ""
msgstr[3] ""

#. A description for an event that repeats daily. For example, Every 2 days
#. [CHAR LIMIT=15]
#, c-format
msgctxt "daily"
msgid "Daily"
msgid_plural "Every <xliff:g id=\"number\">%d</xliff:g> days"
